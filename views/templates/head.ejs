<!-- Code to switch between different titles in browser tab -->
<script>


window.setInterval(function(){
    changetitle()
},10000); //every 10 seconds
let title = document.querySelector('title');
let count = 2;

function changetitle(){
 //change title here

 if(count%2===0) {
    title.innerText = 'Imran says hi';
    console.log(count);
    count++;
  }
   if(count%2!=0) {
    title.innerText = 'DevHoot | Blogs';
    console.log(count);
    count--;
  }
   if(count>1){
     changetitle();
   }
}
</script>